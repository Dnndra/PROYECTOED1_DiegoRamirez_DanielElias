@model PROYECTOED1_DiegoRamirez_DanielElias.Models.Data.Paciente

@{
    ViewData["Title"] = "Login Municipio";
}

<h1>Seleccione el municipio del centro de vacunación</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="LoginMunicipio">
            


            <p> Elija  municipio</p>
            <div class="form-group">
                <select class="form-select" asp-for="Municipio">
                    <option></option>
                    @foreach (var item in PROYECTOED1_DiegoRamirez_DanielElias.Models.Data.Singleton.Instance.muncipios)
                    {

                        <option value="@item">@Html.DisplayFor(modelItem => item)</option>
                    }
                </select>
            </div>

            <div class="form-group">
                <input type="submit" value="Ingresar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


@if (null != TempData["alertMessage"])
{
    <script type="text/javascript">
       alert("@TempData["alertMessage"]");
    </script>
}
